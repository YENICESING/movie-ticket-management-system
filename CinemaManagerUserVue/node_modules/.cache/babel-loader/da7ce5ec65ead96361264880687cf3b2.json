ms: {pageNum: 1, pageSize: 10}\n  }).then(res => {\n    movieList.value = res.data.list\n  })\n}\n\n\nconst searchName = ref('')\n\nfunction search() {\n  openLink('/movieList?name=' + searchName.value)\n}\n\nconst leftAdvertising = ref({})\nconst rightAdvertising = ref({})\n\n\nconst advertisingList = ref([])\ngetAdvertisingList()\n\nfunction getAdvertisingList() {\n  request.get(\"/advertising/list\").then(res => {\n    advertisingList.value = res.data\n    leftAdvertising.value = res.data.find(item => item.position === '轮播图左下侧')\n    rightAdvertising.value = res.data.find(item => item.position === '轮播图右下侧')\n\n  })\n}\n\nfunction openLink(link) {\n  window.location.href = link\n}\n\nconst movieRecommend = ref([])\ngetMovieRecommendList()\n\nfunction getMovieRecommendList() {\n  request.get(\"/movie/recommend/3\").then(res => {\n    movieRecommend.value = res.data\n  })\n}\n\n\n\n</script>\n\n<style scoped>\n.slideshow-card .el-card {\n  --el-card-padding: 0;\n}\n\n.custom-input {\n  border: none; /* 去掉边框 */\n  border-bottom: 2px solid orange; /* 设置底部边框颜色为橙色 */\n  border-radius: 0; /* 去掉圆角 */\n  box-shadow: none; /* 去掉阴影 */\n}\n\n.custom-input .el-input__inner {\n  background-color: transparent; /* 设置背景为透明 */\n  padding: 10px; /* 调整内边距 */\n  color: #333; /* 设置字体颜色 */\n}\n\n.custom-input .el-input__inner:focus {\n  outline: none; /* 去掉聚焦时的轮廓 */\n  border-bottom: 2px solid orange; /* 聚焦时保持橙色边框 */\n}\n</style>\n\n"],"mappings":";;AA4KA,SAAQA,GAAG,EAAEC,KAAK,QAAO,KAAK;AAC9B,OAAOC,OAAO,MAAM,iBAAiB;AACrC,OAAOC,SAAS,MAAM,4BAA4B;AAClD,OAAOC,MAAM,MAAM,mBAAmB;AACtC,SAASC,WAAW,QAAQ,yBAAyB,CAAC,CAAC;;;;;IAGvD,MAAMC,SAAS,GAAGN,GAAG,CAAC,EAAE,CAAC;IACzBO,gBAAgB,CAAC,CAAC;IAGlB,SAASA,gBAAgBA,CAAA,EAAG;MAC1BL,OAAO,CAACM,GAAG,CAAC,iBAAiB,EAAE;QAC7BC,MAAM,EAAE;UAACC,OAAO,EAAE,CAAC;UAAEC,QAAQ,EAAE;QAAE;MACnC,CAAC,CAAC,CAACC,IAAI,CAACC,GAAG,IAAI;QACbP,SAAS,CAACQ,KAAK,GAAGD,GAAG,CAACE,IAAI,CAACC,IAAI;MACjC,CAAC,CAAC;IACJ;IAEA,MAAMC,iBAAiB,GAAGjB,GAAG,CAAC,EAAE,CAAC;IACjCkB,wBAAwB,CAAC
emplate>\n  <div class=\"movie-container\">\n    <!-- 主内容区（包含导航和内容） -->\n    <div class=\"movie-main\">\n      <!-- 集成导航栏 -->\n      <div class=\"integrated-nav\">\n        <el-radio-group v-model=\"activeNav\" class=\"nav-radio-group\">\n          <el-radio-button\n            v-for=\"item in menuList\"\n            :key=\"item.id\"\n            :label=\"item.path\"\n            @click=\"$router.push(item.path)\"\n          >\n            <i :class=\"['nav-icon', item.iconClass]\"></i>\n            <span class=\"nav-title\">{{ item.name }}</span>\n          </el-radio-button>\n        </el-radio-group>\n      </div>\n\n      <!-- 类别筛选区 -->\n      <div class=\"filter-section\">\n        <div class=\"filter-container\">\n          <div class=\"filter-title\">\n            <i class=\"iconfont icon-r-filter\"></i>\n            <span>电影分类</span>\n          </div>\n          <el-radio-group v-model=\"categoryRadio\" class=\"category-group\">\n            <el-radio-button\n              v-for=\"item in [{movieCategoryId: 0, movieCategoryName: '全部'}, ...categoryList]\"\n              :key=\"item.movieCategoryId\"\n              :label=\"item.movieCategoryId\"\n            >\n              {{ item.movieCategoryName }}\n            </el-radio-button>\n          </el-radio-group>\n        </div>\n      </div>\n\n      <!-- 路由视图 -->\n      <router-view\n        :movieCategoryId=\"categoryRadio\"\n        :orderByColumn=\"orderByColumn\"\n      ></router-view>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Movie\",\n  data() {\n    return {\n      activeNav: \"/movie/movieOngoing\", // 默认激活第一个导航\n      menuList: [\n        {\n          id: \"1\",\n          name: \"正在热映\",\n          path: \"/movie/movieOngoing\",\n          iconClass: \"iconfont icon-r-film\"\n        },\n        {\n          id: \"2\",\n          name: \"即将上映\",\n          path: \"/movie/movieUpcoming\",\n          iconClass: \"iconfont icon-r-calendar\"\n        },\n        {\n          id: \"3\",\n          name: \"热播影片\",\n          path: \"/movie/movieClassics\",\n          iconClass: \"iconfont icon-r-fire\"\n        }\n      ],\n      categoryRadio: 0,\n      categoryList: [],\n      orderByColumn: \"releaseDate\"\n    };\n  },\n  created() {\n    this.activeNav = this.$route.path; // 初始化激活状态\n    this.getCategoryList();\n  },\n  methods: {\n    async getCategoryList() {\n      try {\n        const { data: res } = await axios.get(\"sysMovieCategory/find\");\n        if (res.code !== 200) {\n          this.$message.error(\"获取电影分类失败\");\n          return;\n        }\n        this.categoryList = res.data;\n      } catch (error) {\n        this.$message.error(\"请求电影分类出错\");\n        console.error(error);\n      }\n    }\n  },\n  watch: {\n    $route(newRoute) {\n      this.activeNav = newRoute.path;\n      this.categoryRadio = 0;\n      this.orderByColumn = \"releaseDate\";\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.movie-container {\n  height: 100vh;\n  background-color: #f5f7fa;\n  overflow: hidden;\n}\n\n.movie-main {\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n}\n\n/* 集成导航样式 */\n.integrated-nav {\n  .nav-radio-group {\n    .el-radio-button {\n      :deep(.el-radio-button__inner) {\n        width: 100%;\n        border: none !important;\n        background: transparent !important;\n        color: #666;\n        padding: 12px 0;\n        transition: all 0.3s;\n        border-radius: 0 !important;\n        border-bottom: 3px solid transparent !important;\n\n        .nav-icon {\n          font-size: 20px;\n          margin-right: 8px;\n          vertical-align: middle;\n        }\n\n        .nav-title {\n          font-size: 16px;\n          font-weight: 500;\n        }\n      }\n\n      &:not(.is-active):hover {\n        :deep(.el-radio-button__inner) {\n          color: #00ff88;\n          background: rgba(0, 255, 136, 0.05) !important;\n        }\n      }\n\n      &:not(.is-active):hover {\n        ::v-deep (.el-radio-button__inner) {\n          color: #00ff88;\n          background: rgba(0, 255, 136, 0.05) !important;\n        }\n      }\n\n      &.is-active {\n        :deep(.el-radio-button__inner) {\n          color: #00ff88 !important;\n          border-bottom-color: #00ff88 !important;\n          background: rgba(0, 255, 136, 0.1) !important;\n        }\n      }\n    }\n  }\n}\n\n/* 筛选区域样式 */\n.filter-section {\n  background: #ffffff;\n  padding: 15px 20px;\n  margin-bottom: 10px;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);\n\n  .filter-title {\n    display: flex;\n    align-items: center;\n    margin-bottom: 15px;\n\n    i {\n      font-size: 20px;\n      color: #00ff88;\n      margin-right: 10px;\n    }\n\n    span {\n      font-size: 16px;\n      font-weight: 600;\n      color: #333;\n    }\n  }\n}\n\n/* 分类按钮组样式 */\n.category-group {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 10px;\n\n  .el-radio-button {\n    margin-right: 0;\n\n    ::v-deep (.el-radio-button__inner) {\n      border: 1px solid #dcdfe6;\n      border-radius: 20px !important;\n      background: transparent;\n      color: #666;\n      padding: 8px 20px;\n      transition: all 0.3s;\n\n      &:hover {\n        border-color: #00ff88;\n        color: #00ff88;\n      }\n    }\n\n    &.is-active {\n      ::v-deep .el-radio-button__inner {\n        background: #00ff88 !important;\n        border-color: #00ff88 !important;\n        color: #fff !important;\n        box-shadow: 0 2px 8px rgba(0, 255, 136, 0.3) !important;\n        font-weight: 500;\n      }\n    }\n  }\n}\n\n/* 内容区域样式 */\n.content-area {\n  flex: 1;\n  overflow-y: auto;\n  padding: 20px;\n  background: #ffffff;\n}\n\n/* 响应式设计 */\n@media screen and (max-width: 768px) {\n  .integrated-nav {\n    padding: 10px 5px;\n\n    .nav-radio-group {\n      .el-radio-button {\n        ::v-deep (.el-radio-button__inner) {\n          padding: 8px 0;\n\n          .nav-icon {\n            font-size: 18px;\n            margin-right: 5px;\n          }\n\n          .nav-title {\n            font-size: 14px;\n          }\n        }\n      }\n    }\n  }\n\n  .category-group {\n    justify-content: center;\n  }\n}\n</style>\n"]}]}
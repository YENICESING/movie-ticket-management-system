gnment=\"left\" size=\"large\">\n\n\n        <el-row :gutter=\"10\">\n          <el-col :span=\"16\">\n            <el-space direction=\"vertical\" alignment=\"left\" :size=\"10\" style=\"width: 100%\">\n              <el-carousel height=\" 380px\">\n                <el-carousel-item v-for=\"item in slideshow\" :key=\"item\" @click=\"openLink(item.link)\">\n                  <el-image style=\" width: 100%; height: 380px\" fit=\"fill\"\n                            :src=\"item.mainImg\"\n                  />\n                </el-carousel-item>\n\n              </el-carousel>\n\n              <el-row :gutter=\"10\">\n                <el-col :span=\"12\">\n                  <el-image style=\" width: 100%; height: 190px\" fit=\"fill\"\n                            @click=\"openLink(leftAdvertising.link)\"\n                            :src=\"leftAdvertising.mainImg\"\n                  />\n                </el-col>\n\n                <el-col :span=\"12\">\n                  <el-image style=\" width: 100%; height: 190px\" fit=\"fill\"\n                            @click=\"openLink(rightAdvertising.link)\"\n                            :src=\"rightAdvertising.mainImg\"\n                  />\n                </el-col>\n\n              </el-row>\n\n            </el-space>\n\n          </el-col>\n\n          <el-col :span=\"8\">\n            <el-card style=\"height: 580px;\" shadow=\"hover\">\n              <template #header>\n                <div style=\"display: flex; align-items: center; font-weight: bold; color: #EF4238;\">\n                  <el-icon style=\"margin-right: 5px;\"><TrendCharts /></el-icon>\n\n                  实时票房\n                </div>\n\n              </template>\n\n              <div>\n                <div v-for=\"(item,index) in movieBoxOfficeTop\" style=\"cursor: pointer; padding: 8px 0;\"\n                     @click=\"openLink('/movieDetails/'+item.id)\">\n                  <el-space alignment=\"center\">\n                    <div>\n                